<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Nullable>enable</Nullable>
    <TargetFramework>net8.0</TargetFramework>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.CSharp" PrivateAssets="all" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="../../../src/Domain/LeanCode.DomainModels/LeanCode.DomainModels.csproj" />
    <ProjectReference Include="../../../src/Domain/LeanCode.DomainModels.Generators/LeanCode.DomainModels.Generators.csproj" OutputItemType="Analyzer" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.NETCore.App.Ref" Version="8.0.0" IncludeAssets="none" PrivateAssets="all" GeneratePathProperty="true" />
    <EmbeddedResource Include="$(PkgMicrosoft_NETCore_App_Ref)/ref/net8.0/System.Linq.dll">
      <LogicalName>System.Linq</LogicalName>
      <Link>Resources/System.Linq.dll</Link>
    </EmbeddedResource>
    <EmbeddedResource Include="$(PkgMicrosoft_NETCore_App_Ref)/ref/net8.0/System.Linq.Expressions.dll">
      <LogicalName>System.Linq.Expressions</LogicalName>
      <Link>Resources/System.Linq.Expressions.dll</Link>
    </EmbeddedResource>
    <EmbeddedResource Include="$(PkgMicrosoft_NETCore_App_Ref)/ref/net8.0/System.Memory.dll">
      <LogicalName>System.Memory</LogicalName>
      <Link>Resources/System.Memory.dll</Link>
    </EmbeddedResource>
    <EmbeddedResource Include="$(PkgMicrosoft_NETCore_App_Ref)/ref/net8.0/System.Runtime.dll">
      <LogicalName>System.Runtime</LogicalName>
      <Link>Resources/System.Runtime.dll</Link>
    </EmbeddedResource>
    <EmbeddedResource Include="$(PkgMicrosoft_NETCore_App_Ref)/ref/net8.0/System.Text.Json.dll">
      <LogicalName>System.Text.Json</LogicalName>
      <Link>Resources/System.Text.Json.dll</Link>
    </EmbeddedResource>
  </ItemGroup>
</Project>